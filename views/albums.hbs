
   <div class="albums-results">
    {{#each findAlbums}}
     <article class="albums-style">
   <h2> {{this.name}}</h2>
    <img class="artist-photo" src="{{this.images.0.url}}" alt='imageSpotify'>
    <button class="button-artist"><a href="/tracks/{{this.id}}" class="btn btn-primary">View Tracks</a></button>
    </article>
     {{/each}} 
</div>
